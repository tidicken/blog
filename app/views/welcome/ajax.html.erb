<script type="text/javascript">
	function loadBlogPosts(){
		$.getJSON("/blog_posts.json", function(data){
			//console.log(data);

			var html = "";

			$("#blog_posts").html(html); 

			$.each(data, function (index){

				//alert(data[index].subject);

				html += "<b><i>" + data[index].subject + "<i><b><br>";

			});
		alert(html);


		});

		

	}
</script>
<div id= "blog_posts" name="blog_posts">
	<a href="javascript:loadBlogPosts();">This is where blog posts go</a> 
</div>	